{
  "description": "Polymer mixin for localization of D2L components",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceUI/localize-behavior.git"
  },
  "name": "d2l-localize-behavior",
  "scripts": {
    "lint:js": "eslint . --ext .js,.html test/**/*.js test/**/*.html demo/**/*.js demo/**/*.html",
    "lint:wc": "polymer lint",
    "lint": "npm run lint:wc && npm run lint:js",
    "test": "npm run lint && npm run test:polymer:local",
    "test:polymer:local": "polymer test --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --skip-plugin local"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/polyfill": "^7",
    "@webcomponents/webcomponentsjs": "^2",
    "babel-eslint": "^10",
    "eslint": "^6",
    "eslint-config-brightspace": "^0.6",
    "eslint-plugin-html": "^6",
    "eslint-plugin-sort-class-members": "^1",
    "frau-ci": "^1",
    "polymer-cli": "^1",
    "wct-browser-legacy": "^1"
  },
  "version": "2.5.1",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "main": "d2l-localize-behavior.js",
  "dependencies": {
    "@brightspace-ui/core": "^1",
    "@polymer/polymer": "^3"
  }
}
